extends layout

append body
	.pageController(ng-controller="ProfileCtrl" style="margin:0 auto;margin-top:50px")
		.div.userDetailsDiv
			.div.username(style='margin-top: -34px;')
				span.greeting Hey there	
				a(href='/profile/{{username}}'){{user.username}}!
